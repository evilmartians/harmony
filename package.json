{
  "name": "@evilmartians/harmony",
  "version": "1.1.0",
  "scripts": {
    "build": "deno task build",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/index.js",
  "exports": {
    "./tailwind": {
      "import": "./dist/tailwind/index.js",
      "require": "./dist/tailwind/index.mjs",
      "types": "./dist/tailwind/index.d.ts"
    },
    "./base": {
